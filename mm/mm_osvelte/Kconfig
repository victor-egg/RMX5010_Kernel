config OPLUS_FEATURE_MM_OSVELTE
	tristate "osvelte driver"
	default n
	help
	  This driver used for memory debugging.

config OPLUS_FEATURE_MM_OSVELTE_DBG
	tristate "osvelte driver"
	default n
	help
	  This driver used for memory debugging.

config OPLUS_FEATURE_MM_EZRECLAIMD
	bool "Enable ezreclaimd reclaim pages in background"
	default n
	depends on OPLUS_FEATURE_MM_OSVELTE

config KCOMPRESSD
	bool "Enable kcompressed"
	default n
	depends on OPLUS_FEATURE_MM_OSVELTE

config OPLUS_FEATURE_MGLRU_OPT
	bool "Enable mglru opt"
	default n
	depends on OPLUS_FEATURE_MM_OSVELTE
